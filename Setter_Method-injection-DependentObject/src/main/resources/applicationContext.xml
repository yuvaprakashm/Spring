<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="address1" class="com.yuva.Address">
		<property name="addressLane" value="4,RailwayRoad"></property>
		<property name="city" value="KMU"></property>
		<property name="state" value="TN"></property>
		<property name="country" value="India"></property>
	</bean>

	<bean id="obj" class="com.yuva.Employee">
		<property name="id" value="1"></property>
		<property name="name" value="Yuvi"></property>
		<property name="address" ref="address1"></property>
	</bean>

</beans>  